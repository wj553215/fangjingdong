{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\n// useRouter整个大路由信息 useRout访问路径信息\nimport { useRouter, useRoute } from \"vue-router\";\nimport ShopInfo from \"../../components/ShopInfo\";\nimport { get } from \"../../utils/request\";\n\n// 获取当前商品信息\nconst useShopInfoEffrct = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n  const getItemData = async () => {\n    const result = await get(`/api/shop/${route.params.id}`);\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data;\n    }\n  };\n  const {\n    item\n  } = toRefs(data);\n  return {\n    item,\n    getItemData\n  };\n};\nexport default {\n  name: \"Shop\",\n  components: {\n    ShopInfo\n  },\n  setup() {\n    const router = useRouter();\n    const {\n      item,\n      getItemData\n    } = useShopInfoEffrct();\n    // 获取一次数据\n    getItemData();\n    const handleBackClick = () => {\n      router.back();\n    };\n    return {\n      item,\n      handleBackClick\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","useRouter","useRoute","ShopInfo","get","useShopInfoEffrct","route","data","item","getItemData","result","params","id","errno","name","components","setup","router","handleBackClick","back"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\shop\\Shop.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"search\">\r\n      <div class=\"search__back iconfont\" @click=\"handleBackClick\">&#xe600;</div>\r\n      <div class=\"search__content\">\r\n        <span class=\"search__content__icon iconfont\">&#xe62d;</span>\r\n        <input class=\"search__content__input\" placeholder=\"请输入商品名称\" />\r\n      </div>\r\n    </div>\r\n    <ShopInfo :item=\"item\" :hideBorder=\"true\" />\r\n  </div>\r\n</template>\r\n    \r\n  <script>\r\nimport { reactive, toRefs } from \"vue\";\r\n// useRouter整个大路由信息 useRout访问路径信息\r\nimport { useRouter, useRoute } from \"vue-router\";\r\nimport ShopInfo from \"../../components/ShopInfo\";\r\nimport { get } from \"../../utils/request\";\r\n\r\n// 获取当前商品信息\r\nconst useShopInfoEffrct = () => {\r\n  const route = useRoute();\r\n  const data = reactive({ item: {} });\r\n  const getItemData = async () => {\r\n    const result = await get(`/api/shop/${route.params.id}`);\r\n    if (result?.errno === 0 && result?.data) {\r\n      data.item = result.data;\r\n    }\r\n  };\r\n  const{item}=toRefs(data)\r\n  return{item,getItemData}\r\n};\r\n\r\nexport default {\r\n  name: \"Shop\",\r\n  components: { ShopInfo },\r\n  setup() {\r\n    const router = useRouter();\r\n    \r\n    const{item,getItemData}=useShopInfoEffrct();\r\n    // 获取一次数据\r\n    getItemData();\r\n    const handleBackClick = () => {\r\n      router.back();\r\n    };\r\n    return { item, handleBackClick };\r\n  },\r\n};\r\n</script>\r\n    \r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n.wrapper {\r\n  padding: 0 0.18rem;\r\n}\r\n\r\n.search {\r\n  display: flex;\r\n  margin: 0.14rem 0 0.12rem 0;\r\n  line-height: 0.32rem;\r\n\r\n  &__back {\r\n    width: 0.3rem;\r\n    font-size: 0.24rem;\r\n    color: #000;\r\n  }\r\n\r\n  &__content {\r\n    display: flex;\r\n    flex: 1;\r\n    background: $search-bgColor;\r\n    border-radius: 0.16rem;\r\n\r\n    &__icon {\r\n      width: 0.44rem;\r\n      text-align: center;\r\n      color: $search-fontColor;\r\n    }\r\n\r\n    &__input {\r\n      display: block;\r\n      width: 100%;\r\n      padding-right: 0.2rem;\r\n      border: none;\r\n      outline: none;\r\n      background: none;\r\n      height: 0.32rem;\r\n      font-size: 0.14rem;\r\n      color: #333;\r\n\r\n      &::placeholder {\r\n        color: $search-fontColor;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAcA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC;AACA,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAChD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAASC,GAAE,QAAS,qBAAqB;;AAEzC;AACA,MAAMC,iBAAgB,GAAIA,CAAA,KAAM;EAC9B,MAAMC,KAAI,GAAIJ,QAAQ,EAAE;E