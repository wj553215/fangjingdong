{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRoute } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects';\nimport { ref } from 'vue';\nimport { post } from '../../utils/request';\nexport default {\n  name: 'Order',\n  setup() {\n    const route = useRoute();\n    const shopId = parseInt(route.params.id, 10);\n    const {\n      calculations,\n      shopName\n    } = useCommonCartEffect(shopId);\n    const order = ref(false);\n    const handleOrder = () => {\n      order.value = !order.value;\n    };\n    const handleConfremOrder = async () => {\n      const products = [];\n      for (const i in products.value) {\n        const product = products.value[i];\n        products.push({\n          id: product._id,\n          mun: product.count\n        });\n      }\n      try {\n        const result = await post('/api/order', {\n          addressId: 1,\n          shopId,\n          shopName: shopName.value,\n          isCanceled: false,\n          products\n        });\n        if (result?.errno === 0) {\n          // localStorage.isLogin = true\n          // // 告诉路由登录后从新跳转\n          route.push({\n            name: 'Home'\n          });\n        }\n      } catch (e) {\n        // showToast('请求失败')\n      }\n    };\n    return {\n      calculations,\n      order,\n      handleOrder,\n      handleConfremOrder\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useCommonCartEffect","ref","post","name","setup","route","shopId","parseInt","params","id","calculations","shopName","order","handleOrder","value","handleConfremOrder","products","i","product","push","_id","mun","count","result","addressId","isCanceled","errno","e"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\orderConfirmation\\Order.vue"],"sourcesContent":["<template>\r\n  <div class=\"order\">\r\n    <div class=\"order__price\">实付金额<b>&yen; {{ calculations.price }}</b></div>\r\n    <div class=\"order__btn\" @click=\"handleOrder\">提交订单</div>\r\n  </div>\r\n  <div class=\"mask\" v-if=\"order\">\r\n    <div class=\"mask__content\">\r\n      <h3 class=\"mask__content__title\">确认要离开收银台？</h3>\r\n      <p class=\"mask__content__desc\">请尽快完成支付，否则将被取消</p>\r\n      <div class=\"mask__content__btns\">\r\n        <div class=\"mask__content__btn mask__content__btn--first\" @click=\"handleOrder\">取消订单</div>\r\n        <div class=\"mask__content__btn mask__content__btn--last\" @click=\"handleConfremOrder\">确认支付</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { useRoute } from 'vue-router'\r\nimport { useCommonCartEffect } from '../../effects/cartEffects'\r\nimport { ref } from 'vue'\r\nimport { post } from '../../utils/request'\r\nexport default {\r\n  name: 'Order',\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = parseInt(route.params.id, 10)\r\n    const { calculations, shopName } = useCommonCartEffect(shopId)\r\n    const order = ref(false)\r\n    const handleOrder = () => {\r\n      order.value = !order.value\r\n    }\r\n\r\n    const handleConfremOrder = async () => {\r\n      const products = []\r\n      for (const i in products.value) {\r\n        const product = products.value[i]\r\n        products.push({ id: product._id, mun: product.count })\r\n      }\r\n      try {\r\n        const result = await post('/api/order', {\r\n          addressId: 1,\r\n          shopId,\r\n          shopName: shopName.value,\r\n          isCanceled: false,\r\n          products\r\n        })\r\n        if (result?.errno === 0) {\r\n          // localStorage.isLogin = true\r\n          // // 告诉路由登录后从新跳转\r\n          route.push({ name: 'Home' })\r\n        }\r\n      } catch (e) {\r\n        // showToast('请求失败')\r\n      }\r\n    }\r\n    return { calculations, order, handleOrder, handleConfremOrder }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n\r\n.order {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  display: flex;\r\n  height: 0.49rem;\r\n  line-height: 0.49rem;\r\n  background: $bgColor;\r\n\r\n  &__price {\r\n    flex: 1;\r\n    text-indent: .24rem;\r\n    font-size: .14rem;\r\n    color: $content-fontcolor;\r\n  }\r\n\r\n  &__btn {\r\n    width: 0.89rem;\r\n    background: #4FB0F9;\r\n    font-size: 14px;\r\n    color: $bgColor;\r\n    text-align: center;\r\n  }\r\n}\r\n\r\n.mask {\r\n  z-index: 1;\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.50);\r\n\r\n  &__content {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    width: 3rem;\r\n    height: 1.57rem;\r\n    background: #fff;\r\n    border-radius: .04rem;\r\n    text-align: center;\r\n\r\n    &__title {\r\n      margin-top: .24rem;\r\n      margin-bottom: 0;\r\n      line-height: .26rem;\r\n      font-size: .18rem;\r\n      color: #333;\r\n    }\r\n\r\n    &__desc {\r\n      margin-top: .08rem;\r\n      line-height: .21rem;\r\n      font-size: 14px;\r\n      color: #666;\r\n    }\r\n\r\n    &__btns {\r\n      display: flex;\r\n      margin: .24rem .58rem;\r\n    }\r\n\r\n    &__btn {\r\n      flex: 1;\r\n      width: 0.8rem;\r\n      line-height: .32rem;\r\n      font-size: 14px;\r\n      border-radius: .16rem;\r\n\r\n      &--first {\r\n        margin-right: .24rem;\r\n        border: .01rem solid #4FB0F9;\r\n        color: #0091FF;\r\n      }\r\n\r\n      &--last {\r\n        background: #4FB0F9;\r\n        color: #fff;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAiBA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,mBAAkB,QAAS,2BAA0B;AAC9D,SAASC,GAAE,QAAS,KAAI;AACxB,SAASC,IAAG,QAAS,qBAAoB;AACzC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,MAAMO,MAAK,GAAIC,QAAQ,CAACF,KAAK,CAACG,MAAM,CAACC,EAAE,EAAE,EAAE;IAC3C,MAAM;MAAEC,YAAY;MAAEC;IAAS,IAAIX,mBAAmB,CAACM,MAAM;IAC7D,MAAMM,KA