(_cache[2] = $event => $setup.ensurement = $event)\n  }, null, 512), [[_vModelText, $setup.ensurement]])]), _createElementVNode(\"div\", {\n    class: \"wrapper__register-button\",\n    onClick: _cache[3] || (_cache[3] = (...args) => $setup.handleRegister && $setup.handleRegister(...args))\n  }, \"注册\"), _createElementVNode(\"div\", {\n    class: \"wrapper__register-link\",\n    onClick: _cache[4] || (_cache[4] = (...args) => $setup.handleLoginClick && $setup.handleLoginClick(...args))\n  }, \" 已有账号去登录 \"), $setup.show ? (_openBlock(), _createBlock(_component_Toast, {\n    key: 0,\n    message: $setup.toastMessage\n  }, null, 8, [\"message\"])) : _createCommentVNode(\"\", true)]);\n}","map":{"version":3,"names":["class","_createElementVNode","src","_createElementBlock","_hoisted_1","_hoisted_2","_hoisted_3","placeholder","$setup","username","$event","_hoisted_4","type","password","autocomplete","_hoisted_5","ensurement","onClick","_cache","args","handleRegister","handleLoginClick","show","_createBlock","_component_Toast","message","toastMessage"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\register\\Register.vue"],"sourcesContent":["<!-- eslint-disable no-empty -->\r\n<!-- 注册 -->\r\n<template>\r\n  <div class=\"wrapper\">\r\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\" />\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入手机号码\" v-model=\"username\" />\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入密码\" type=\"password\" v-model=\"password\"\r\n        autocomplete=\"new-password\" />\r\n      <!-- autocomplete=\"new-password\"不自动填充 -->\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"确认密码\" type=\"password\" v-model=\"ensurement\" />\r\n    </div>\r\n    <div class=\"wrapper__register-button\" @click=\"handleRegister\">注册</div>\r\n    <div class=\"wrapper__register-link\" @click=\"handleLoginClick\">\r\n      已有账号去登录\r\n    </div>\r\n    <Toast v-if=\"show\" :message=\"toastMessage\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request'\r\nimport Toast, { useToastEffect } from '../../components/Toast'\r\n\r\nconst useRegisterEffect = (showToast) => {\r\n  // 路由\r\n  const router = useRouter()\r\n  // 绑定输入框的值\r\n  const data = reactive({ username: '', password: '', ensurement: '' })\r\n  const handleRegister = async () => {\r\n    try {\r\n      const { username, password, ensurement } = data\r\n      if (username === '' || password === '' || ensurement === '') {\r\n        showToast('账号或密码不能为空')\r\n        localStorage.isLogin = false\r\n        return\r\n      } else if (password !== ensurement) {\r\n        showToast('两次密码不一致')\r\n        localStorage.isLogin = false\r\n        return\r\n        // eslint-disable-next-line no-empty\r\n      } else { };\r\n      const result = await post('/api/user/register', {\r\n        uername: data.username,\r\n        password: data.password\r\n      })\r\n      if (result?.errno === 0) {\r\n        // 告诉路由登录后从新跳转\r\n        router.push({ name: 'Login' })\r\n      } else {\r\n        showToast('注册失败')\r\n      }\r\n    } catch (e) {\r\n      showToast('请求失败')\r\n    }\r\n  }\r\n  const { username, password, ensurement } = toRefs(data)\r\n  return { username, password, ensurement, handleRegister }\r\n}\r\nexport default {\r\n  name: 'Register',\r\n  components: { Toast },\r\n  setup () {\r\n    const router = useRouter()\r\n    const { show, toastMessage, showToast } = useToastEffect()\r\n    const { username, password, ensurement, handleRegister } =\r\n      useRegisterEffect(showToast)\r\n\r\n    const handleLoginClick = () => {\r\n      router.push({ name: 'Login' })\r\n    }\r\n    return {\r\n      username,\r\n      password,\r\n      ensurement,\r\n      toastMessage,\r\n      show,\r\n      handleLoginClick,\r\n      handleRegister\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n\r\n  &__img {\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    width: .66rem;\r\n    height: .66rem;\r\n  }\r\n\r\n  &__input {\r\n    box-sizing: border-box;\r\n    height: .48rem;\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    background: #f9f9f9 100%;\r\n    border: 1rem solid rgba(0, 0, 0, 0.1);\r\n    border-radius: .06rem;\r\n\r\n    &__content {\r\n      line-height: .48rem;\r\n      border: 0;\r\n      outline: none;\r\n      width: 100%;\r\n      background: none;\r\n      font-size: .16rem;\r\n      color: #000000;\r\n\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__register-button {\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    font-size: .16rem;\r\n    text-align: center;\r\n    background: $btn-bgColor;\r\n    box-shadow: 0 .04rem .08rem 0 rgba(0, 145, 255, 0.32);\r\n    border-radius: .04rem;\r\n    color: $bgColor;\r\n  }\r\n\r\n  &__register-link {\r\n    text-align: center;\r\n    font-size: .14rem;\r\n    color: $content-notice-fontcolor;\r\n    letter-spacing: 0;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;EAGOA,KAAK,EAAC;AAAS;gEAClBC,mBAAA,CAA6E;EAAxED,KAAK,EAAC,cAAc;EAACE,GAAG,EAAC;;;EACzBF,KAAK,EAAC;AAAgB;;EAGtBA,KAAK,EAAC;AAAgB;;EAKtBA,KAAK,EAAC;AAAgB;;;uBAV7BG,mBAAA,CAkBM,OAlBNC,UAkBM,GAjBJC,UAA6E,EAC7EJ,mBAAA,CAEM,OAFNK,UAEM,G,gBADJL,mBAAA,CAAkF;IAA3ED,KAAK,EAAC,yBAAyB;IAACO,WAAW,EAAC,SAAS;+DAAUC,MAAA,CAAAC,QAAQ,GAAAC,MAAA;gCAARF,MAAA,CAAAC,QAAQ,E,KAEhFR,mBAAA,CAIM,OAJNU,UAIM,G,gBAHJV,mBAAA,CACgC;IADzBD,KAAK,EAAC,yBAAyB;IAACO,WAAW,EAAC,OAAO;IAACK,IAAI,EAAC,UAAU;+DAAUJ,MAAA,CA