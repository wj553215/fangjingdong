yle/mixins.scss\";\n.wrapper {\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 0;\n  bottom: 0.5rem;\n  padding: 0 0.18rem 0.1rem 0.18rem;\n}\n.position {\n  position: relative;\n  padding: 0.16rem 0.24rem 0.16rem 0;\n  line-height: 0.22rem;\n  font-size: 0.16rem;\n  color: $content-fontcolor;\n  // 省略文字\n  @include ellipsis;\n  .position__icon {\n    position: relative;\n    top: 0.03rem;\n    margin-right: 0.1rem;\n    font-size: 0.2rem;\n  }\n  .position__notice {\n    position: absolute;\n    right: 0;\n    margin-top: 0.02rem;\n    font-size: 0.2rem;\n  }\n}\n.search {\n  margin-bottom: 0.12rem;\n  // 行高自动撑开\n  line-height: 0.32rem;\n  background: #f5f5f5;\n  color: #b7b7b7;\n  // 圆角\n  border-radius: 0.16rem;\n  font-size: 14px;\n  .iconfont {\n    position: relative;\n    top: 0.02rem;\n    display: inline-block;\n    padding: 0 0.12rem 0 0.16rem;\n  }\n  .input {\n    position: relative;\n    padding: 0 1.3rem 0 0;\n    //去除默认边框和选中边框\n    border: 0px;\n    outline: none;\n    @include ellipsis;\n    background: #f5f5f5;\n    color: #131313;\n  }\n}\n.banner {\n  //防止抖动（撑开图片的高度）\n  height: 0;\n  overflow: hidden;\n  padding-bottom: 25.4%;\n  &__img {\n    width: 100%;\n  }\n}\n.icons {\n  //弹性布局\n  display: flex;\n  flex-wrap: wrap;\n  margin-top: .16rem;\n  &__item {\n    width: 20%;\n    &__img {\n      display: block;\n      width: 0.4rem;\n      height: 0.4rem;\n      margin: 0 auto;\n    }\n    &__desc {\n      margin: 0.06rem 0 0.16rem 0;\n      text-align: center;\n      color: $content-fontcolor;\n    }\n  }\n}\n.gap{\n  margin: 0 -.18rem;\n  height: 0.1rem;\n  background:$content-bgColor;\n}\n.nearby{\n  &__title{\n    margin: .16rem 0 .14rem .18rem;\n    font-size: .18rem;\n    font-weight: normal;\n    color: $content-fontcolor;\n  }\n  &__item{\n    display: flex;\n    padding-top: .12rem ;\n    &__img{\n      width: .56rem;\n      height: .56rem;\n      margin-top: 0.1rem;\n      margin-right: .16rem;\n    }\n  }\n  &__content{\n    flex: