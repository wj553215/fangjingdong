{"ast":null,"code":"import { ref } from \"vue\";\nimport { get } from \"../../utils/request\";\nimport ShopInfo from \"../../components/ShopInfo.vue\";\nconst useNearbyListEffect = () => {\n  // 接口数据\n  const nearbyList = ref([]);\n  const getNearbyList = async () => {\n    const result = await get(\"/api/shop/hot-list\");\n    if (result?.errno === 0 && result?.data.length) {\n      nearbyList.value = result.data;\n    }\n  };\n  return {\n    nearbyList,\n    getNearbyList\n  };\n};\nexport default {\n  name: \"Nearby\",\n  // 引入ShopInfo\n  components: {\n    ShopInfo\n  },\n  setup() {\n    const {\n      nearbyList,\n      getNearbyList\n    } = useNearbyListEffect();\n    getNearbyList();\n    return {\n      nearbyList\n    };\n  }\n};","map":{"version":3,"names":["ref","get","ShopInfo","useNearbyListEffect","nearbyList","getNearbyList","result","errno","data","length","value","name","components","setup"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\home\\Nearby.vue"],"sourcesContent":["<!-- 附近店铺 -->\r\n<template>\r\n  <div class=\"nearby\">\r\n    <h3 class=\"nearby__title\">附近店铺</h3>\r\n    <router-link\r\n    v-for=\"item in nearbyList\" \r\n    :key=\"item._id\"\r\n    :to=\"`/Shop/${iten._id}`\"\r\n    >\r\n    <ShopInfo :item=\"item\"/>\r\n  </router-link>\r\n    <!-- :item=\"item\"将数据传出 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\nimport { get } from \"../../utils/request\";\r\nimport ShopInfo from \"../../components/ShopInfo.vue\";\r\n\r\nconst useNearbyListEffect = () => {\r\n  // 接口数据\r\n  const nearbyList = ref([]);\r\n  const getNearbyList = async () => {\r\n    const result = await get(\"/api/shop/hot-list\");\r\n    if (result?.errno === 0 && result?.data.length) {\r\n      nearbyList.value = result.data;\r\n    }\r\n  };\r\n  return { nearbyList, getNearbyList };\r\n};\r\nexport default {\r\n  name: \"Nearby\",\r\n  // 引入ShopInfo\r\n  components:{ShopInfo},\r\n  setup() {\r\n    const { nearbyList, getNearbyList } = useNearbyListEffect();\r\n    getNearbyList();\r\n    return { nearbyList };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n// 颜色引用\r\n@import \"../../style/viriables.scss\";\r\n\r\n.nearby {\r\n  &__title {\r\n    margin: 0.16rem 0 0.14rem 0.18rem;\r\n    font-size: 0.18rem;\r\n    font-weight: normal;\r\n    color: $content-fontcolor;\r\n  }\r\n  a{\r\n    text-decoration: none;\r\n  }\r\n}\r\n</style>"],"mappings":"AAgBA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,GAAE,QAAS,qBAAqB;AACzC,OAAOC,QAAO,MAAO,+BAA+B;AAEpD,MAAMC,mBAAkB,GAAIA,CAAA,KAAM;EAChC;EACA,MAAMC,UAAS,GAAIJ,GAAG,CAAC,EAAE,CAAC;EAC1B,MAAMK,aAAY,GAAI,MAAAA,CAAA,KAAY;IAChC,MAAMC,MAAK,GAAI,MAAML,GAAG,CAAC,oBAAoB,CAAC;IAC9C,IAAIK,MAAM,EAAEC,KAAI,KAAM,KAAKD,MAAM,EAAEE,IAAI,CAACC,MAAM,EAAE;MAC9CL,UAAU,CAACM,KAAI,GAAIJ,MAAM,CAACE,IAAI;IAChC;EACF,CAAC;EACD,OAAO;IAAEJ,UAAU;IAAEC;EAAc,CAAC;AACtC,CAAC;AACD,eAAe;EACbM,IAAI,EAAE,QAAQ;EACd;EACAC,UAAU,EAAC;IAACV;EAAQ,CAAC;EACrBW,KAAKA,CAAA,EAAG;IACN,MAAM;MAAET,UAAU;MAAEC;IAAc,IAAIF,mBAAmB,EAAE;IAC3DE,aAAa,EAAE;IACf,OAAO;MAAED;IAAW,CAAC;EACvB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}