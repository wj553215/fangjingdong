{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n//引入实例\nimport { useRouter } from \"vue-router\";\nimport { post } from '../../utils/request';\nimport { reactive } from \"vue\";\naxios.defaults.headers.post[\"Content-Type\"] = \"application/json\";\nexport default {\n  name: \"Login\",\n  setup() {\n    // 绑定输入框的值\n    const data = reactive({\n      username: \"\",\n      password: \"\"\n    });\n    // 获取到路由的节点\n    const router = useRouter();\n    const handleLogin = async () => {\n      try {\n        const result = await axios.post(\"https://www.fastmock.site/mock/ae8e9031947a302fed5f92425995aa19/jd/api/user/login\", {\n          uername: data.username,\n          password: data.password\n        });\n        if (result?.data?.errno === 0) {\n          localStorage.isLogin = true;\n          // 告诉路由登录后从新跳转\n          router.push({\n            name: \"Home\"\n          });\n        } else {\n          alert(\"登录失败\");\n        }\n      } catch (e) {\n        alert('请求失败');\n      }\n    };\n    const handleRegisterClick = () => {\n      router.push({\n        name: \"Register\"\n      });\n    };\n    return {\n      handleLogin,\n      handleRegisterClick,\n      data\n    };\n  }\n};","map":{"version":3,"names":["useRouter","post","reactive","axios","defaults","headers","name","setup","data","username","password","router","handleLogin","result","uername","errno","localStorage","isLogin","push","alert","e","handleRegisterClick"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<!-- 登录 -->\r\n<template>\r\n  <div class=\"wrapper\">\r\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\" />\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入用户名\" v-model=\"data.username\" />\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入密码\" type=\"password\" v-model=\"data.password\" />\r\n    </div>\r\n    <div class=\"wrapper__login-button\" @click=\"handleLogin\">登录</div>\r\n    <div class=\"wrapper__login-link\" @click=\"handleRegisterClick\">立即注册</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//引入实例\r\nimport { useRouter } from \"vue-router\";\r\nimport {post} from '../../utils/request';\r\nimport { reactive } from \"vue\";\r\n\r\naxios.defaults.headers.post[\"Content-Type\"] = \"application/json\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  setup() {\r\n    // 绑定输入框的值\r\n    const data = reactive({\r\n      username: \"\",\r\n      password: \"\",\r\n    });\r\n    // 获取到路由的节点\r\n    const router = useRouter();\r\n    const handleLogin = async () => {\r\n      try {\r\n        const result = await axios.post(\"https://www.fastmock.site/mock/ae8e9031947a302fed5f92425995aa19/jd/api/user/login\",\r\n          {\r\n            uername: data.username,\r\n            password: data.password,\r\n          })\r\n        if (result?.data?.errno === 0) {\r\n          localStorage.isLogin = true;\r\n          // 告诉路由登录后从新跳转\r\n          router.push({ name: \"Home\" });\r\n        } else {\r\n          alert(\"登录失败\");\r\n        }\r\n      }catch(e){\r\n        alert('请求失败')\r\n      }\r\n    };\r\n    const handleRegisterClick = () => {\r\n      router.push({ name: \"Register\" });\r\n    };\r\n    return { handleLogin, handleRegisterClick, data };\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n\r\n  &__img {\r\n    display: block;\r\n    margin: 0 auto 0.4rem auto;\r\n    width: 0.66rem;\r\n    height: 0.66rem;\r\n  }\r\n\r\n  &__input {\r\n    box-sizing: border-box;\r\n    height: 0.48rem;\r\n    margin: 0 0.4rem 0.16rem 0.4rem;\r\n    padding: 0 0.16rem;\r\n    background: #f9f9f9 100%;\r\n    border: 1px solid rgba(0, 0, 0, 0.1);\r\n    border-radius: 6px;\r\n\r\n    &__content {\r\n      line-height: 0.48rem;\r\n      border: 0;\r\n      outline: none;\r\n      width: 100%;\r\n      background: none;\r\n      font-size: 0.16rem;\r\n      color: #000000;\r\n\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__login-button {\r\n    margin: 0.32rem 0.4rem 0.16rem 0.4rem;\r\n    line-height: 0.48rem;\r\n    font-size: 0.16rem;\r\n    text-align: center;\r\n    background: #0091ff;\r\n    box-shadow: 0 0.04rem 0.08rem 0 rgba(0, 145, 255, 0.32);\r\n    border-radius: 4px;\r\n    color: #ffff;\r\n  }\r\n\r\n  &__login-link {\r\n    text-align: center;\r\n    font-size: 0.14rem;\r\n    color: $content-notice-fontcolor;\r\n    letter-spacing: 0;\r\n  }\r\n}\r\n</style>"],"mappings":";AAgBA;AACA,SAASA,SAAQ,QAAS,YAAY;AACtC,SAAQC,IAAI,QAAO,qBAAqB;AACxC,SAASC,QAAO,QAAS,KAAK;AAE9BC,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACJ,IAAI,CAAC,cAAc,IAAI,kBAAkB;AAEhE,eAAe;EACbK,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN;IACA,MAAMC,IAAG,GAAIN,QAAQ,CAAC;MACpBO,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC,CAAC;IACF;IACA,MAAMC,MAAK,GAAIX,SAAS,EAAE;IAC1B,MAAMY,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,MAAK,GAAI,MAAMV,KAAK,CAACF,IAAI,CAAC,mFAAmF,EACjH;UACEa,OAAO,EAAEN,IAAI,CAACC,QAAQ;UACtBC,QAAQ,EAAEF,IAAI,CAACE;QACjB,CAAC;QACH,IAAIG,MAAM,EAAEL,IAAI,EAAEO,KAAI,KAAM,CAAC,EAAE;UAC7BC,YAAY,CAACC,OAAM,GAAI,IAAI;UAC3B;UACAN,MAAM,CAACO,IAAI,CAAC;YAAEZ,IAAI,EAAE;UAAO,CAAC,CAAC;QAC/B,OAAO;UACLa,KAAK,CAAC,MAAM,CAAC;QACf;MACF,CAAC,QAAMC,CAAC,EAAC;QACPD,KAAK,CAAC,MAAM;MACd;IACF,CAAC;IACD,MAAME,mBAAkB,GAAIA,CAAA,KAAM;MAChCV,MAAM,CAACO,IAAI,CAAC;QAAEZ,IAAI,EAAE;MAAW,CAAC,CAAC;IACnC,CAAC;IACD,OAAO;MAAEM,WAAW;MAAES,mBAAmB;MAAEb;IAAK,CAAC;EACnD;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}