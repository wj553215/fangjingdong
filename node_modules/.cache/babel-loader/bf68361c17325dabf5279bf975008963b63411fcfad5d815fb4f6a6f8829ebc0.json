{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { computed, ref } from 'vue';\nimport Docker from '../../components/Docker.vue';\nimport { useCommonCartEffect } from '../../effects/cartEffects';\nexport default {\n  name: 'CartList',\n  components: {\n    Docker\n  },\n  setup() {\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      productList,\n      shopName\n    } = useCommonCartEffect(shopId);\n    const quantity = computed(() => {\n      const result = {\n        item: 0,\n        displayedItems: 1\n      };\n      for (const i in productList.value) {\n        const product = productList.value[i];\n        result.displayedItems = result.displayedItems + 1;\n        if (product.check) {\n          result.item = result.item + 1;\n        }\n      }\n      return result;\n    });\n    const displayedItems = ref(3);\n    const handleDisplay = () => {\n      if (displayedItems.value === 3) {\n        displayedItems.value = quantity.value.displayedItems;\n      } else {\n        displayedItems.value = 3;\n      }\n    };\n    return {\n      productList,\n      shopName,\n      quantity,\n      handleDisplay,\n      displayedItems\n    };\n  }\n};","map":{"version":3,"names":["useRoute","computed","ref","Docker","useCommonCartEffect","name","components","setup","route","shopId","params","id","productList","shopName","quantity","result","item","displayedItems","i","value","product","check","handleDisplay"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\cartList\\CartList.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"title\">我的全部购物车</div>\r\n    <div class=\"products\">\r\n      <div class=\"products__title\">沃尔玛</div>\r\n      <div class=\"products__list\">\r\n        <template v-for=\"(item, index) in  productList \" :key=\"item._id\">\r\n          <div class=\"products__item\" v-if=\"item.check && index < displayedItems\">\r\n            <img class=\"products__item__img\" :src=\"item.imgUrl\" />\r\n            <div class=\"products__item__detail\">\r\n              <h4 class=\"products__item__title\">{{ item.name }}</h4>\r\n              <p class=\"products__item__price\">\r\n                <span class=\"products__item__yen\">&yen;{{ item.price }} x {{ item.count\r\n                }}</span>\r\n                <span class=\"products__item__origin\">&yen;{{ (item.price * item.count).toFixed(2) }}</span>\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </template>\r\n        <div class=\"products__list__btn\">\r\n          <span class=\"products__list__btn__text\" @click=\"handleDisplay\">共计{{ quantity.item }}件/1.4kg</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!-- 底栏 -->\r\n  <Docker :currentIndex=\"1\" />\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from 'vue-router'\r\nimport { computed, ref } from 'vue'\r\nimport Docker from '../../components/Docker.vue'\r\nimport { useCommonCartEffect } from '../../effects/cartEffects'\r\n\r\nexport default {\r\n  name: 'CartList',\r\n  components: { Docker },\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { productList, shopName } = useCommonCartEffect(shopId)\r\n\r\n    const quantity = computed(() => {\r\n      const result = { item: 0, displayedItems: 1 }\r\n      for (const i in productList.value) {\r\n        const product = productList.value[i]\r\n        result.displayedItems = result.displayedItems + 1\r\n        if (product.check) { result.item = result.item + 1 }\r\n      }\r\n      return result\r\n    })\r\n\r\n    const displayedItems = ref(3)\r\n    const handleDisplay = () => {\r\n      if (displayedItems.value === 3) {\r\n        displayedItems.value = quantity.value.displayedItems\r\n      } else { displayedItems.value = 3 }\r\n    }\r\n    return { productList, shopName, quantity, handleDisplay, displayedItems }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n\r\n.wrapper {\r\n  //纵向滚动\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 0;\r\n  bottom: .5rem;\r\n  background: #f8f8f8;\r\n}\r\n\r\n.title {\r\n  line-height: .44rem;\r\n  text-align: center;\r\n  font-size: .16rem;\r\n  color: #333;\r\n  background: #fff;\r\n}\r\n\r\n.products {\r\n  margin: .16rem .18rem .1rem .18rem;\r\n  background: $bgColor;\r\n\r\n  &__title {\r\n    position: relative;\r\n    line-height: .54rem;\r\n    padding: .16rem .16rem .16rem .16rem;\r\n    font-size: .16rem;\r\n    color: $content-fontcolor;\r\n  }\r\n\r\n  &__list {\r\n    overflow-y: scroll;\r\n    position: absolute;\r\n    margin: 0 .18rem;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: .6rem;\r\n    top: 1.14rem;\r\n\r\n    &__btn {\r\n      position: relative;\r\n      background: #FFFFFF;\r\n      padding: 0 .16rem .16rem .16rem;\r\n\r\n      &__text {\r\n        display: block;\r\n        line-height: .28rem;\r\n        font-size: .14rem;\r\n        color: #999999;\r\n        text-align: center;\r\n        background: #F5F5F5;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__item {\r\n    background: $bgColor;\r\n    position: relative;\r\n    display: flex;\r\n    padding: 0 .16rem .16rem .16rem;\r\n\r\n    &__img {\r\n      width: .46rem;\r\n      height: .46rem;\r\n      margin-right: .16rem;\r\n    }\r\n\r\n    &__detail {\r\n      flex: 1;\r\n    }\r\n\r\n    &__title {\r\n      margin: 0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color: $content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n\r\n    &__price {\r\n      display: flex;\r\n      position: relative;\r\n      margin: .06rem 0 0 0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color: $hightlight-fontColor;\r\n    }\r\n\r\n    &__yen {\r\n      margin: .06rem 0 0 0;\r\n      text-align: right;\r\n    }\r\n\r\n    &__origin {\r\n      position: absolute;\r\n      margin: .06rem 0 0 0;\r\n      right: .16rem;\r\n      text-align: left;\r\n      color: #000;\r\n    }\r\n  }\r\n\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,QAAQ,EAAEC,GAAE,QAAS,KAAI;AAClC,OAAOC,MAAK,MAAO,6BAA4B;AAC/C,SAASC,mBAAkB,QAAS,2BAA0B;AAE9D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEH;EAAO,CAAC;EACtBI,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAIR,QAAQ,EAAC;IACvB,MAAMS,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,EAAC;IAC7B,MAAM;MAAEC,WAAW;MAAEC;IAAS,IAAIT,mBAAmB,CAACK,MAAM;IAE5D,MAAMK,QAAO,GAAIb,QAAQ,CAAC,MAAM;MAC9B,MAAMc,MAAK,GAAI;QAAEC,IAAI,EAAE,CAAC;QAAEC,cAAc,EAAE;MAAE;MAC5C,KAAK,MAAMC,CAAA,IAAKN,WAAW,CAACO,KAAK,EAAE;QACjC,MAAMC,OAAM,GAAIR,WAAW,CAACO,KAAK,CAACD,CAAC;QACnCH,MAAM,CAACE,cAAa,GAAIF,MAAM,CAACE,cAAa,GAAI;QAChD,IAAIG,OAAO,CAACC,KAAK,EAAE;UAAEN,MAAM,CAACC,IAAG,GAAID,MAAM,CAACC,IAAG,GAAI;QAAE;MACrD;MACA,OAAOD,MAAK;IACd,CAAC;IAED,MAAME,cAAa,GAAIf,GAAG,CAAC,CAAC;IAC5B,MAAMoB,aAAY,GAAIA,CAAA,KAAM;MAC1B,IAAIL,cAAc,CAACE,KAAI,KAAM,CAAC,EAAE;QAC9BF,cAAc,CAACE,KAAI,GAAIL,QAAQ,CAACK,KAAK,CAACF,cAAa;MACrD,OAAO;QAAEA,cAAc,CAACE,KAAI,GAAI;MAAE;IACpC;IACA,OAAO;MAAEP,WAAW;MAAEC,QAAQ;MAAEC,QAAQ;MAAEQ,aAAa;MAAEL;IAAe;EAC1E;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}