e) => {\r\n      toastData.showToast = true;\r\n      toastData.toastMessage = message;\r\n      setTimeout(() => {\r\n        data.showToast = false;\r\n        data.toastMessage = \"\";\r\n      }, 2000);\r\n    };\r\n};\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  components: { Toast },\r\n  setup() {\r\n    // 获取到路由的节点\r\n    const router = useRouter();\r\n    // 绑定输入框的值\r\n    const data = reactive({\r\n      username: \"\",\r\n      password: \"\",\r\n    });\r\n    const handleLogin = async () => {\r\n      try {\r\n        const result = await post(\"/api/user/login\", {\r\n          uername: data.username,\r\n          password: data.password,\r\n        });\r\n        if (result?.errno === 0) {\r\n          localStorage.isLogin = true;\r\n          // 告诉路由登录后从新跳转\r\n          router.push({ name: \"Home\" });\r\n        } else {\r\n          showToast(登录失败);\r\n        }\r\n      } catch (e) {\r\n        showToast(请求失败);\r\n      }\r\n    };\r\n    const handleRegisterClick = () => {\r\n      router.push({ name: \"Register\" });\r\n    };\r\n    return { handleLogin, handleRegisterClick, data };\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n\r\n  &__img {\r\n    display: block;\r\n    margin: 0 auto 0.4rem auto;\r\n    width: 0.66rem;\r\n    height: 0.66rem;\r\n  }\r\n\r\n  &__input {\r\n    box-sizing: border-box;\r\n    height: 0.48rem;\r\n    margin: 0 0.4rem 0.16rem 0.4rem;\r\n    padding: 0 0.16rem;\r\n    background: #f9f9f9 100%;\r\n    border: 1px solid rgba(0, 0, 0, 0.1);\r\n    border-radius: 6px;\r\n\r\n    &__content {\r\n      line-height: 0.48rem;\r\n      border: 0;\r\n      outline: none;\r\n      width: 100%;\r\n      background: none;\r\n      font-size: 0.16rem;\r\n      color: #000000;\r\n\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__login-button {\r\n    margin: 0.32rem 0.4rem 0.16rem 0.4rem;\r\n    line-height: 0.48rem;\r\n    font-size: 0.16rem;\r\n    text-align: center;\r\n    background: #0091ff;\r\n    box-shadow: 0 0.04rem 0.08rem 0 rgba(0, 145, 255, 0.32);\r\n    border-radius: 4px;\r\n    color: #ffff;\r\n  }\r\n\r\n  &__login-link {\r\n    text-align: center;\r\n    font-size: 0.14rem;\r\n    color: $content-notice-fontcolor;\r\n    letter-spacing: 0;\r\n  }\r\n}\r\n</style>"],"mappings":";;;EAEOA,KAAK,EAAC;AAAS;gEAClBC,mBAAA,CAGE;EAFAD,KAAK,EAAC,cAAc;EACpBE,GAAG,EAAC;;;EAEDF,KAAK,EAAC;AAAgB;;EAOtBA,KAAK,EAAC;AAAgB;;;uBAZ7BG,mBAAA,CAuBM,OAvBNC,UAuBM,GAtBJC,UAGE,EACFJ,mBAAA,CAMM,OANNK,UAMM,G,gBALJL,mBAAA,CAIE;IAHAD,KAAK,EAAC,yBAAyB;IAC/BO,WAAW,EAAC,QAAQ;+DACXC,MAAA,CAAAC,IAAI,CAACC,QAAQ,GAAAC,MAAA;iDAAbH,MAAA,CAAAC,IAAI,CAACC,QAAQ,E,KAG1BT,mBAAA,CAOM,OAPNW,UAOM,G,gBANJX,mBAAA,CAKE;IAJAD,KAAK,EAAC,yBAAyB;IAC/BO,WAAW,EAAC,OAAO;IACnBM,IAAI,EAAC,UAAU;+DACNL,MAAA,CAAAC,IAAI,CAACK,QAAQ,GAAAH,MAAA;iDAAbH,MAAA,CAAAC,IAAI,CAACK,QAAQ,E,KAG1Bb,mBAAA,CAAgE;IAA3DD,KAAK,EAAC,uBAAuB;IAAEe,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAET,MAAA,CAAAU,WAAA,IAAAV,MAAA,CAAAU,WAAA,IAAAD,IAAA,CAAW;KAAE,IAAE,GAC1DhB,mBAAA,CAAwE;IAAnED,KAAK,EAAC,qBAAqB;IAAEe,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAET,MAAA,CAAAW,mBAAA,IAAAX,MAAA,CAAAW,mBAAA,IAAAF,IAAA,CAAmB;KAAE,MAAI,GACrDT,MAAA,CAAAC,IAAI,CAACW,SAAS,I,cAA3BC,YAAA,CAA4DC,gBAAA;;IAA9BC,OAAO,EAAEf,MAAA,CAAAC,IAAI,CAACe"},"metadata":{},"sourceType":"module","externalDependencies":[]}