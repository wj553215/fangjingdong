{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\nimport { get } from \"../../utils/request\";\nexport default {\n  name: \"Content\",\n  setup() {\n    const data = reactive({\n      contentList: []\n    });\n    const getContentData = async () => {\n      const result = await get(\"/api/shop/:id/products\", {\n        tab: \"all\"\n      });\n      if (result?.errno === 0 && result?.data?.length) {\n        data.contentList = result.data;\n      }\n      getContentData();\n      const {\n        contentList\n      } = toRefs(data);\n      return {\n        contentList\n      };\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","get","name","setup","data","contentList","getContentData","result","tab","errno","length"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\shop\\Content.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"category\">\r\n      <div class=\"category__item category__item--active\">全部商品1</div>\r\n      <div class=\"category__item\">全部商品2</div>\r\n      <div class=\"category__item\">全部商品3</div>\r\n    </div>\r\n    <div class=\"product\">\r\n      <div \r\n      class=\"product__item\" \r\n      v-for=\"(item,index) in contentList\"\r\n      :key=\"index\"\r\n      >\r\n        <img\r\n          class=\"product__item__img\"\r\n          src=\"http://www.dell-lee.com/imgs/vue3/user.png\"\r\n        />\r\n        <div class=\"product__item__detail\">\r\n          <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n          <p class=\"product__item__sales\">月售 {{ item.sales }} </p>\r\n          <p class=\"product__item__price\">\r\n            <span class=\"product__iteme__yen\">&yen;33.6</span>\r\n            <span class=\"product__item__origin\">&yen;33.6</span>\r\n          </p>\r\n        </div>\r\n        <div class=\"product__number\">\r\n          <span class=\"product__number__minus\">-</span>\r\n          88\r\n          <span class=\"product__number__plus\">+</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from \"vue\";\r\nimport { get } from \"../../utils/request\";\r\nexport default {\r\n  name: \"Content\",\r\n  setup() {\r\n    const data = reactive({ contentList: [] });\r\n    const getContentData = async () => {\r\n      const result = await get(\"/api/shop/:id/products\", {\r\n        tab: \"all\",\r\n      });\r\n      if (result?.errno === 0 && result?.data?.length) {\r\n        data.contentList = result.data;\r\n      }\r\n      getContentData();\r\n      const {contentList}=toRefs(data);\r\n      return{contentList};\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n@import \"../../style/mixins.scss\";\r\n.content {\r\n  // 超出滚动\r\n  display: flex;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 1.5rem;\r\n  bottom: 0.5rem;\r\n}\r\n.category {\r\n  overflow-y: scroll;\r\n  height: 100%;\r\n  width: 0.76rem;\r\n  background: $search-bgColor;\r\n  &__item {\r\n    line-height: 0.4rem;\r\n    text-align: center;\r\n    font-size: 0.14rem;\r\n    color: $content-fontcolor;\r\n    &--active {\r\n      background: $bgColor;\r\n    }\r\n  }\r\n}\r\n.product {\r\n  overflow: scroll;\r\n  flex: 1;\r\n  margin-top: 0.12rem;\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: 0.12rem 0;\r\n    margin: 0 0.16rem;\r\n    border-bottom: 0.01rem solid $content-bgColor;\r\n    &__detail {\r\n      overflow: hidden;\r\n    }\r\n    &__img {\r\n      width: 0.68rem;\r\n      height: 0.68rem;\r\n      margin-right: 0.16rem;\r\n    }\r\n    &__title {\r\n      //   display: flex;\r\n      margin: 0 0 0.06rem;\r\n      line-height: 0.2rem;\r\n      font-size: 14px;\r\n      color: $content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__sales {\r\n      margin: 0 0 0.06rem;\r\n      font-size: 0.12rem;\r\n      color: $content-fontcolor;\r\n      line-height: 0.16rem;\r\n    }\r\n    &__price {\r\n      line-height: 0.2rem;\r\n      font-size: 0.14rem;\r\n      color: #e93b3b;\r\n    }\r\n    &__yen {\r\n      font-size: 0.12rem;\r\n    }\r\n    &__origin {\r\n      margin-left: 0.06rem;\r\n      font-size: 0.12rem;\r\n      line-height: 0.2rem;\r\n      color: #999;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number {\r\n      position: absolute;\r\n      right: 0;\r\n      bottom: 0.12rem;\r\n\r\n      &__minus,\r\n      &__plus {\r\n        display: inline-block;\r\n        width: 0.2rem;\r\n        height: 0.2rem;\r\n        line-height: 0.17rem;\r\n        border-radius: 50%;\r\n        font-size: 0.2rem;\r\n        text-align: center;\r\n      }\r\n      &__minus {\r\n        border: 0.01rem solid #666;\r\n        color: $content-fontcolor;\r\n        margin-right: 0.07rem;\r\n      }\r\n      &__plus {\r\n        background: #0091ff;\r\n        color: $bgColor;\r\n        margin-left: 0.07rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style> "],"mappings":"AAoCA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,SAASC,GAAE,QAAS,qBAAqB;AACzC,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIL,QAAQ,CAAC;MAAEM,WAAW,EAAE;IAAG,CAAC,CAAC;IAC1C,MAAMC,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjC,MAAMC,MAAK,GAAI,MAAMN,GAAG,CAAC,wBAAwB,EAAE;QACjDO,GAAG,EAAE;MACP,CAAC,CAAC;MACF,IAAID,MAAM,EAAEE,KAAI,KAAM,KAAKF,MAAM,EAAEH,IAAI,EAAEM,MAAM,EAAE;QAC/CN,IAAI,CAACC,WAAU,GAAIE,MAAM,CAACH,IAAI;MAChC;MACAE,cAAc,EAAE;MAChB,MAAM;QAACD;MAAW,CAAC,GAACL,MAAM,CAACI,IAAI,CAAC;MAChC,OAAM;QAACC;MAAW,CAAC;IACrB,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}