{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, toRefs } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { post } from '../../utils/request';\nimport Toast, { useToastEffect } from '../../components/Toast';\nconst useRegisterEffect = showToast => {\n  // 路由\n  const router = useRouter();\n  // 绑定输入框的值\n  const data = reactive({\n    username: '',\n    password: '',\n    ensurement: ''\n  });\n  const handleRegister = async () => {\n    try {\n      const {\n        username,\n        password,\n        ensurement\n      } = data;\n      if (username === '' || password === '' || ensurement === '') {\n        showToast('账号或密码不能为空');\n        localStorage.isLogin = false;\n        return;\n      } else if (password !== ensurement) {\n        showToast('两次密码不一致');\n        localStorage.isLogin = false;\n        return;\n        // eslint-disable-next-line no-empty\n      } else {}\n      ;\n      const result = await post('/api/user/register', {\n        uername: data.username,\n        password: data.password\n      });\n      if (result?.errno === 0) {\n        // 告诉路由登录后从新跳转\n        router.push({\n          name: 'Login'\n        });\n      } else {\n        showToast('注册失败');\n      }\n    } catch (e) {\n      showToast('请求失败');\n    }\n  };\n  const {\n    username,\n    password,\n    ensurement\n  } = toRefs(data);\n  return {\n    username,\n    password,\n    ensurement,\n    handleRegister\n  };\n};\nexport default {\n  name: 'Register',\n  components: {\n    Toast\n  },\n  setup() {\n    const router = useRouter();\n    const {\n      show,\n      toastMessage,\n      showToast\n    } = useToastEffect();\n    const {\n      username,\n      password,\n      ensurement,\n      handleRegister\n    } = useRegisterEffect(showToast);\n    const handleLoginClick = () => {\n      router.push({\n        name: 'Login'\n      });\n    };\n    return {\n      username,\n      password,\n      ensurement,\n      toastMessage,\n      show,\n      handleLoginClick,\n      handleRegister\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","useRouter","post","Toast","useToastEffect","useRegisterEffect","showToast","router","data","username","password","ensurement","handleRegister","localStorage","isLogin","result","uername","errno","push","name","e","components","setup","show","toastMessage","handleLoginClick"],"sources":["D:\\WEB\\project\\jingdong\\src\\views\\register\\Register.vue"],"sourcesContent":["<!-- eslint-disable no-empty -->\r\n<!-- 注册 -->\r\n<template>\r\n  <div class=\"wrapper\">\r\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\" />\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入手机号码\" v-model=\"username\" />\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"请输入密码\" type=\"password\" v-model=\"password\"\r\n        autocomplete=\"new-password\" />\r\n      <!-- autocomplete=\"new-password\"不自动填充 -->\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input class=\"wrapper__input__content\" placeholder=\"确认密码\" type=\"password\" v-model=\"ensurement\" />\r\n    </div>\r\n    <div class=\"wrapper__register-button\" @click=\"handleRegister\">注册</div>\r\n    <div class=\"wrapper__register-link\" @click=\"handleLoginClick\">\r\n      已有账号去登录\r\n    </div>\r\n    <Toast v-if=\"show\" :message=\"toastMessage\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request'\r\nimport Toast, { useToastEffect } from '../../components/Toast'\r\n\r\nconst useRegisterEffect = (showToast) => {\r\n  // 路由\r\n  const router = useRouter()\r\n  // 绑定输入框的值\r\n  const data = reactive({ username: '', password: '', ensurement: '' })\r\n  const handleRegister = async () => {\r\n    try {\r\n      const { username, password, ensurement } = data\r\n      if (username === '' || password === '' || ensurement === '') {\r\n        showToast('账号或密码不能为空')\r\n        localStorage.isLogin = false\r\n        return\r\n      } else if (password !== ensurement) {\r\n        showToast('两次密码不一致')\r\n        localStorage.isLogin = false\r\n        return\r\n        // eslint-disable-next-line no-empty\r\n      } else { };\r\n      const result = await post('/api/user/register', {\r\n        uername: data.username,\r\n        password: data.password\r\n      })\r\n      if (result?.errno === 0) {\r\n        // 告诉路由登录后从新跳转\r\n        router.push({ name: 'Login' })\r\n      } else {\r\n        showToast('注册失败')\r\n      }\r\n    } catch (e) {\r\n      showToast('请求失败')\r\n    }\r\n  }\r\n  const { username, password, ensurement } = toRefs(data)\r\n  return { username, password, ensurement, handleRegister }\r\n}\r\nexport default {\r\n  name: 'Register',\r\n  components: { Toast },\r\n  setup () {\r\n    const router = useRouter()\r\n    const { show, toastMessage, showToast } = useToastEffect()\r\n    const { username, password, ensurement, handleRegister } =\r\n      useRegisterEffect(showToast)\r\n\r\n    const handleLoginClick = () => {\r\n      router.push({ name: 'Login' })\r\n    }\r\n    return {\r\n      username,\r\n      password,\r\n      ensurement,\r\n      toastMessage,\r\n      show,\r\n      handleLoginClick,\r\n      handleRegister\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n\r\n  &__img {\r\n    display: block;\r\n    margin: 0 auto 0.4rem auto;\r\n    width: 0.66rem;\r\n    height: 0.66rem;\r\n  }\r\n\r\n  &__input {\r\n    box-sizing: border-box;\r\n    height: 0.48rem;\r\n    margin: 0 0.4rem 0.16rem 0.4rem;\r\n    padding: 0 0.16rem;\r\n    background: #f9f9f9 100%;\r\n    border: 1rem solid rgba(0, 0, 0, 0.1);\r\n    border-radius: .06rem;\r\n\r\n    &__content {\r\n      line-height: 0.48rem;\r\n      border: 0;\r\n      outline: none;\r\n      width: 100%;\r\n      background: none;\r\n      font-size: 0.16rem;\r\n      color: #000000;\r\n\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__register-button {\r\n    margin: 0.32rem 0.4rem 0.16rem 0.4rem;\r\n    line-height: 0.48rem;\r\n    font-size: 0.16rem;\r\n    text-align: center;\r\n    background: $btn-bgColor;\r\n    box-shadow: 0 0.04rem 0.08rem 0 rgba(0, 145, 255, 0.32);\r\n    border-radius: .04rem;\r\n    color: $bgColor;\r\n  }\r\n\r\n  &__register-link {\r\n    text-align: center;\r\n    font-size: 0.14rem;\r\n    color: $content-notice-fontcolor;\r\n    letter-spacing: 0;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAyBA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAI;AACrC,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,IAAG,QAAS,qBAAoB;AACzC,OAAOC,KAAK,IAAIC,cAAa,QAAS,wBAAuB;AAE7D,MAAMC,iBAAgB,GAAKC,SAAS,IAAK;EACvC;EACA,MAAMC,MAAK,GAAIN,SAAS,EAAC;EACzB;EACA,MAAMO,IAAG,GAAIT,QAAQ,CAAC;IAAEU,QAAQ,EAAE,EAAE;IAAEC,QAAQ,EAAE,EAAE;IAAEC,UAAU,EAAE;EAAG,CAAC;EACpE,MAAMC,cAAa,GAAI,MAAAA,CAAA,KAAY;IACjC,IAAI;MACF,MAAM;QAAEH,QAAQ;QAAEC,QAAQ;QAAEC;MAAW,IAAIH,IAAG;MAC9C,IAAIC,QAAO,KAAM,EAAC,IAAKC,QAAO,KAAM,EAAC,IAAKC,UAAS,KAAM,EAAE,EAAE;QAC3DL,SAAS,CAAC,WAAW;QACrBO,YAAY,CAACC,OAAM,GAAI,KAAI;QAC3B;MACF,OAAO,IAAIJ,QAAO,KAAMC,UAAU,EAAE;QAClCL,SAAS,CAAC,SAAS;QACnBO,YAAY,CAACC,OAAM,GAAI,KAAI;QAC3B;QACA;MACF,OAAO,CAAE;MAAC;MACV,MAAMC,MAAK,GAAI,MAAMb,IAAI,CAAC,oBAAoB,EAAE;QAC9Cc,OAAO,EAAER,IAAI,CAACC,QAAQ;QACtBC,QAAQ,EAAEF,IAAI,CAACE;MACjB,CAAC;MACD,IAAIK,MAAM,EAAEE,KAAI,KAAM,CAAC,EAAE;QACvB;QACAV,MAAM,CAACW,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAQ,CAAC;MAC/B,OAAO;QACLb,SAAS,CAAC,MAAM;MAClB;IACF,EAAE,OAAOc,CAAC,EAAE;MACVd,SAAS,CAAC,MAAM;IAClB;EACF;EACA,MAAM;IAAEG,QAAQ;IAAEC,QAAQ;IAAEC;EAAW,IAAIX,MAAM,CAACQ,IAAI;EACtD,OAAO;IAAEC,QAAQ;IAAEC,QAAQ;IAAEC,UAAU;IAAEC;EAAe;AAC1D;A